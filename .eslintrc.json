{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "import/prefer-default-export": "off",
        "no-underscore-dangle": "off",
        "no-useless-retur": "off"
    }
}